
@{
    ViewBag.Title = "NewSelectOnes";
    Layout = "~/Views/Show/Show.cshtml";
}

<h2>NewSelectOnes</h2>

@model List<ParKingMVC.Models.NewsModel>
<table class="layui-table layui-table-body" style="padding-left:450px; background-color:skyblue;width:980px ;">
    <tr>
        <td>新闻编号</td>
        <td>标题</td>
        <td>内容</td>
        <td>发布日期</td>
        <td>浏览量</td>
    </tr>
    @foreach (var m in Model)
    {
        <tr>
            <td>@Model.First().NID</td>
            <td style="width:30px;height:25px">@Model.First().Npicture</td>
            <td>@Model.First().Nname</td>
            <td>@Model.First().Nnei</td>
            <td>@Model.First().Lui</td>
        </tr>
    }
</table>
